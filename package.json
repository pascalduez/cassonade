{
  "name": "cassonade",
  "version": "0.1.0",
  "description": "Sass utilities",
  "keywords": [
    "Sass",
    "utilities"
  ],
  "author": {
    "name": "Pascal Duez",
    "url": "https://github.com/pascalduez"
  },
  "homepage": "https://github.com/pascalduez/cassonade",
  "bugs": "https://github.com/pascalduez/cassonade/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/pascalduez/cassonade.git"
  },
  "license": "Unlicense",
  "files": [
    "index.scss",
    "scss",
    "CHANGELOG.md",
    "README.md"
  ],
  "main": "./index.sss",
  "exports": {
    ".": "./index.scss"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "format:check": "prettier --check --cache --cache-location=.cache/prettier/cache .",
    "format:write": "prettier --write --cache --cache-location=.cache/prettier/cache .",
    "test": "node --test",
    "docs": "sassdoc src/",
    "build": "pnpm /^build:.*/"
  },
  "packageManager": "pnpm@8.6.12",
  "devDependencies": {
    "fast-glob": "^3.3.1",
    "prettier": "^3.0.2",
    "sass": "^1.66.1",
    "sass-true": "^7.0.0",
    "sassdoc": "^2.7.4"
  }
}
